{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "üöÄ Launch Chrome for CyberConnect",
      "url": "http://localhost:5000",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/src/*"
      },
      "preLaunchTask": "Dev Server"
    },
    {
      "type": "chrome",
      "request": "attach",
      "name": "üîó Attach to Chrome",
      "port": 9222,
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true
    },
    {
      "type": "firefox",
      "request": "launch",
      "name": "ü¶ä Launch Firefox",
      "url": "http://localhost:5000",
      "webRoot": "${workspaceFolder}",
      "pathMappings": [
        {
          "url": "webpack:///src/",
          "path": "${webRoot}/src/"
        }
      ]
    },
    {
      "type": "msedge",
      "request": "launch",
      "name": "üåê Launch Edge",
      "url": "http://localhost:5000",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true
    }
  ]
}
