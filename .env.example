# Example Environment Variables for CyberConnect
# Copy this file to .env.local and fill in your actual values

# API Configuration
VITE_API_BASE_URL=https://api.cyberconnect.io
VITE_WS_BASE_URL=wss://api.cyberconnect.io
VITE_APP_ENV=development

# Bug Bounty Platform Integration
# Get these from your platform account settings
VITE_HACKERONE_API_KEY=your_hackerone_api_key_here
VITE_BUGCROWD_API_KEY=your_bugcrowd_api_key_here
VITE_INTIGRITI_API_KEY=your_intigriti_api_key_here
VITE_YESWEHACK_API_KEY=your_yeswehack_api_key_here

# Cloud Provider Keys (for VM provisioning)
VITE_AWS_ACCESS_KEY_ID=your_aws_access_key
VITE_AWS_SECRET_ACCESS_KEY=your_aws_secret_key
VITE_AWS_REGION=us-east-1
VITE_DIGITALOCEAN_TOKEN=your_do_token
VITE_GCP_PROJECT_ID=your_gcp_project

# Threat Intelligence APIs
VITE_SHODAN_API_KEY=your_shodan_api_key
VITE_VIRUSTOTAL_API_KEY=your_virustotal_api_key

# GitHub Integration
VITE_GITHUB_CLIENT_ID=your_github_client_id
VITE_GITHUB_CLIENT_SECRET=your_github_client_secret

# Push Notifications
VITE_VAPID_PUBLIC_KEY=your_push_notification_key

# Analytics (optional)
VITE_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX

# Error Tracking (optional)
VITE_SENTRY_DSN=your_sentry_dsn

# Development Settings
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug